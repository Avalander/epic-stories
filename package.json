{
  "name": "epic-stories",
  "version": "1.0.0-beta.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "mkdir -p static && rm -rf static/* && npm run parse-sw mode=production",
    "build": "webpack --env=prod",
    "postbuild": "./scripts/pre-deploy.sh",
    "preclient": "rm -rf ./static-dev && mkdir static-dev && npm run parse-sw mode=development",
    "client": "webpack-dev-server --env=dev --mode=development",
    "server": "NODE_PATH=server:shared node server/index.js",
    "start": "nf start",
    "parse-sw": "node ./scripts/build-sw.js",
    "test": "npm run test:web",
    "test:web": "NODE_PATH=web:shared:web/__test__ tape -r babel-register 'web/**/*.test.js' | tap-diff",
    "e2e:prepare": "npm run build && ./scripts/pre-e2e.sh",
    "e2e:start": "cd .e2e && ./scripts/start.sh",
    "e2e:stop": "cd .e2e && ./scripts/stop.sh",
    "e2e:relaunch": "npm run e2e:stop && npm run e2e:prepare && npm run e2e:start",
    "cypress:open": "cypress open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@avalander/fun": "^1.0.0-beta.2",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@hyperapp/fx": "^0.7.0",
    "@hyperapp/html": "^1.1.1",
    "@hyperapp/router": "^0.7.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "cypress": "^3.1.0",
    "file-loader": "^2.0.0",
    "font-awesome": "^4.7.0",
    "foreman": "^3.0.1",
    "html-webpack-plugin": "^4.0.0-alpha.2",
    "hyperapp": "^1.2.9",
    "marked": "^0.5.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "tap-diff": "^0.1.1",
    "tape": "^4.9.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@hyperapp/transitions": "^1.0.2",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-history-api-fallback": "^2.2.1",
    "fluture": "^10.1.0",
    "jsonwebtoken": "^8.3.0",
    "mongodb": "^3.1.4"
  }
}
